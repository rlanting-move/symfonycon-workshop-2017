  "require-dev": {
    "behat/behat": "^3.3",
    "phpunit/phpunit": "^6.3",
    "symfony/browser-kit": "^3.3",
    "symfony/dotenv": "^3.3"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
