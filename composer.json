{
  "name": "symfonycon/mastermind",
  "description": "Lightning Fast Tests Workshop",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Zalas",
      "email": "jakub@zalas.pl"
    }
  ],
  "autoload": {
    "psr-4": {"SymfonyCon\\Mastermind\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"SymfonyCon\\Mastermind\\": "tests/"}
  },
  "require": {
    "php": "^7.1.3",
    "doctrine/doctrine-bundle": "^1.7",
    "doctrine/orm": "^2.5",
    "ramsey/uuid": "^3.7",
    "sensio/framework-extra-bundle": "^5.0",
    "symfony/css-selector": "^3.3",
    "symfony/flex": "^1.0.41",
    "symfony/form": "^3.3",
    "symfony/framework-bundle": "^3.3",
    "symfony/twig-bundle": "^3.3",
    "symfony/validator": "^3.3",
    "symfony/yaml": "^3.3"
  },
  "require-dev": {
    "behat/behat": "^3.3",
    "phpunit/phpunit": "^6.3",
    "symfony/browser-kit": "^3.3",
    "symfony/dotenv": "^3.3",
    "symfony/web-server-bundle": "^3.3"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "id": "01BTB20H6C71G1VDE93VNJR662",
      "allow-contrib": false
    },
    "config-dir": "src/Adapters/Web/Resources/config"
  }
}
